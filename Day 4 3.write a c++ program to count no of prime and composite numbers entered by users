#include<iostream>
using namespace std;
int main(){
	int a[10],n,p=0,c=0;
	cout<<"Enter 10 numbers:";
	cin>>n;
	for(int i=0;i<10;i++)
	{
		cin>>a[i];
		if(a[i]==2 || a[i]==5 || a[i]==3 || a[i]==7)
		{
			p++;
		}
		else if(a[i]%2==0 || a[i]%3==0 || a[i]%5==0 || a[i]%7==0)
		{
			c++;
		}
		else
		{
			p++;
		}
	}
	cout<<"\n no prime numbers:"<<p<<"\nno of composite numbers:"<<c;
}
